{
  "name": "react-cornerstone-viewport-hooks",
  "version": "1.7.0",
  "description": "react cornerstone viewport component (with hooks)",
  "author": "wheresvic",
  "license": "MIT",
  "repository": "git@github.com:usb-radiology/react-cornerstone-viewport-hooks.git",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs --css-modules false",
    "start": "microbundle-crl watch --no-compress --format modern,cjs --css-modules false",
    "prepare": "run-s build",
    "test": "run-s test:lint test:mocha",
    "test:mocha": "mocha --require esm 'src/**/*.test.js'",
    "test:lint": "eslint .",
    "predeploy": "cd example && npm install && npm run build"
  },
  "peerDependencies": {
    "cornerstone-core": "2.3.x",
    "cornerstone-math": "0.1.x",
    "cornerstone-tools": "5.x.x",
    "cornerstone-wado-image-loader": "3.3.x",
    "hammerjs": "2.0.8",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "dependencies": {
    "date-fns": "2.22.1",
    "dicom-parser": "1.8.7",
    "npm-run-all": "4.1.5"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "chai": "4.3.4",
    "cornerstone-core": "2.3.0",
    "cornerstone-math": "0.1.9",
    "cornerstone-tools": "5.1.4",
    "cornerstone-wado-image-loader": "3.3.1",
    "cross-env": "7.0.3",
    "eslint": "7.28.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-config-standard-react": "11.0.1",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.24.0",
    "esm": "3.2.25",
    "hammerjs": "2.0.8",
    "microbundle-crl": "0.13.11",
    "mocha": "9.1.1",
    "prettier": "2.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "files": [
    "dist"
  ]
}
